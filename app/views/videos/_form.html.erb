<div class="white_text">

  <%= simple_form_for @video do |f| %>
    <%= f.input :title %>
    <%= f.association :console, prompt:('Choisir la console') %>
    <%= f.input :link %>
    <%= f.input :style %>
    <%= f.input :release, start_year: Date.today.year - 150, end_year: Date.today.year %>
    <%= f.input :genre %>
    <%= f.input :player %>
    <%= f.input :developer %>
    <%= f.file_field :video_img %>
    <%= f.button :submit %>
  <% end %>

</div>
