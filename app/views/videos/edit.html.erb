<div class="white_text">

  <h1>Modifier la vid√©o</h1>

  <%= simple_form_for @video, :html => { :multipart => true } do |f| %>
    <%= f.input :title %>
    <%= f.association :console, prompt:('Choisir la console') %>
    <%= f.input :link %>
    <%= f.input :style %>
    <%= f.input :release, start_year: Date.today.year - 110, end_year: Date.today.year %>
    <%= f.input :genre %>
    <%= f.input :player %>
    <%= f.input :developer %>
    <%= f.file_field :video_img %>
    <%= f.button :submit %>
  <% end %>

</div>
